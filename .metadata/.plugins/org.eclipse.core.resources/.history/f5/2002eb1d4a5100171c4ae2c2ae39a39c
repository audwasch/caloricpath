package com.caloricpath.client;

public class Point
{
	private int m_x;
	private int m_y;
	
	public Point(int x, int y)
	{
		setX(x);
		setY(y);
	}
	
	public void setX(int x)
	{
		m_x = x;
	}
	
	public void setY(int y)
	{
		m_y = y;
	}
	
	public int getX()
	{
		return m_x;
	}
	
	public int getY()
	{
		return m_y;
	}
}
